{
  "name": "YoutubeSync",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_title": "YoutubeSync"
  },
  "background": {
    "scripts": ["./background/socket.io.js", "./background/background.js"]
  },
  "permissions": ["<all_urls>"]
}
